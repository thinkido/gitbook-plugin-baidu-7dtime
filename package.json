{
  "name": "gitbook-plugin-baidu-7dtime",
  "description": "Baidu Analytics tracking for your gitbook version 3",
  "main": "index.js",
  "version": "1.0.0",
  "engines": {
    "gitbook": ">=3.0.0"
  },
  "homepage": "https://github.com/thinkido/gitbook-plugin-baidu-7dtime",
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkido/gitbook-plugin-baidu-7dtime.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thinkido/gitbook-plugin-baidu-7dtime/issues"
  },
  "gitbook": {
    "properties": {
      "token": {
        "type": "string",
        "title": "Baidu Analytics Token",
        "required": true
      },
      "configuration": {
        "type": ["string", "object"],
        "title": "Configuration mode",
        "default": "auto"
      }
    }
  }
}